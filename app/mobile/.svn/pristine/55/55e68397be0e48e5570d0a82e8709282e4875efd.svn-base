	<script type="text/javascript">
		(function($){
			var owlSlider = $('#slider_owl');

			owlSlider.on('changed.owl.carousel', function(event) {
				/*
					Callback 이벤트 (슬라이드가 실행 될 떄마다 실행되는 함수)
					* index 사용하기
					event.item.index

					* length 사용하기
					event.item.count
				*/
			});

			owlSlider.owlCarousel({
				stagePadding: 0,
				margin:0,
				items : 1,
				merge: true,
				loop : true,
				navigation: true,
				nav: true,
				autoplay:true,
			});
			$('.owl-prev').html('<img src="/images/reebok/about/classic/dashonic/arrow_prev.png" alt="">');
			$('.owl-next').html('<img src="/images/reebok/about/classic/dashonic/arrow_next.png" alt="">');


			setTimeout(function(){
				$('.main_section').addClass('on');
			},1000);

			var rolling = setInterval(mainslide,3500)
			var index = 1;
			function mainslide(){
				if(index ==0){
					$('.slider_wrap .slide .item').removeClass('on').eq(index).addClass('on');
					$('.slider_wrap .indicator span').removeClass('on').eq(index).addClass('on');
					index =1;
				}else{

					$('.slider_wrap .slide .item').removeClass('on').eq(index).addClass('on');
					$('.slider_wrap .indicator span').removeClass('on').eq(index).addClass('on');
					index = 0;
				}
			}
			$('.slider_wrap .indicator span').click(function(){
				index = $(this).index();
				$('.slider_wrap .slide .item').removeClass('on').eq(index).addClass('on');
				$('.slider_wrap .indicator span').removeClass('on').eq(index).addClass('on');
			});

			$('.play_btn').bind('click',function(){
				// alert('COMING SOON');

				var ytbId = $(this).attr('data-video-id');
				var html = '<div id="video-layer" style="">';
				html += '	<div class="bg" ></div>';
				html += '	<div class="video-box"></div>';
				html += '	<a href="javascript:void(0)" class="close_x_btn white"></a>';
				html += '</div>';

				$('html').css({overflow: 'hidden'});
				$('body').append(html);

				$('#video-layer .close_x_btn').bind('click',function(){
					$('html').css({overflow: 'scroll'});
					$('#video-layer ').remove();
				});

				setTimeout(function(){
					$('#video-layer').addClass('loaded');

					$('#video-layer .video-box').append('<iframe width="100%" height="100%" src="http://www.youtube.com/embed/'+ytbId+'?rel=0" frameborder="0" allowfullscreen></iframe>')
				},100);

			});

			var tag = document.createElement('script');

			tag.src = "https://www.youtube.com/iframe_api";
			var firstScriptTag = document.getElementsByTagName('script')[0];
			firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

			var player;
			function loadVideo(videoId,targetId) {
			player = new YT.Player(targetId, {
					height: '100%',
					width: '100%',
					videoId: videoId,
					playerVars: {
						controls: 0,
						showinfo : 0,
						autohide : 0,
						disablekb : 0,
						loop : 1,
						modestbranding : 1,
						rel : 0
					}
			});

			player.playVideo();
			}

			// 4. The API will call this function when the video player is ready.
			function onPlayerReady(event) {
			event.target.playVideo();
			}

			// 5. The API calls this function when the player's state changes.
			var done = false;
			function onPlayerStateChange(event) {
				if (event.data == YT.PlayerState.PLAYING && !done) {
					setTimeout(stopVideo, 6000);
					done = true;
				}
			}
			function stopVideo() {
				player.stopVideo();
			}

			var scrollArray = [];
			var scrollContent = document.querySelectorAll('.scrollbox');
			for(var i = 0; i<scrollContent.length; i++){
				 var mypos = scrollContent[i].offsetTop;
				 scrollArray.push(mypos)
			};

			$(window).ready(function(){

				var scrollArray = [];
				var scrollContent = document.querySelectorAll('.scrollbox');
				for(var i = 0; i<scrollContent.length; i++){
					 var mypos = scrollContent[i].offsetTop;
					 scrollArray.push(mypos)
				};

				$(window).scroll(function(){
					var thispos = $(this).scrollTop() + 350;
					for(var pos = 0; pos<scrollArray.length; pos++){
						if(thispos>scrollArray[pos]){
							$('.tamin_campaign .scrollbox').eq(pos).addClass('on');
						}
					}
				});

			});



		})(jQuery);
	</script>

