				var renderMiProducts = function(data , viewLength){
					var i = 0;
					var counter = 0;
					var htmlString = '<div class="item_container">';
					var attrId;
					var elemContainer = document.getElementById('miproducts_wrapper');

					for(; i < data.length; i++){
						if(data[i].isHidden){
							continue;
						}

						attrId = 'item_img'+(Math.floor(i / viewLength) + '_' + counter);
						counter += 1;


						htmlString += '<div class="item" id="'+attrId+'">';
						htmlString += '		<a href="'+data[i].link+'" title="'+data[i].name+'">';
						htmlString += '			<div class="img"><div class="loading-container"><div class="loader-icon"></div></div><img src="'+data[i].imgUrl+'" alt=""></div>';
						htmlString += '			<div class="item_info">';
						htmlString += '				<div class="text">'+data[i].name+'</div>';
						htmlString += '			</div>';
						htmlString += '		</a>';
						htmlString += '</div>';

						if(i == data.length-1){
							htmlString += '</div>';
						}else if(counter == viewLength){
							if(data[i+1].isHidden && i+1 == data.length-1){
								htmlString += '</div>';
							}else{
								htmlString += '</div><div class="item_container">';
								counter = 0;
							}
						}

						(function(attrId , i , imgPath){
							setTimeout(function(){
								document.getElementById(attrId).className = document.getElementById(attrId).className + ' loaded';
							} , i * 60);

							var img = new Image();
							img.src = imgPath;
							img.onload = function(){
								document.querySelector('#'+attrId+' .img img').className = 'loaded';
								document.querySelector('#'+attrId+' .img').removeChild(document.querySelector('#'+attrId+' .img .loading-container'));
							};
						})(attrId , i , data[i].imgUrl);
					}

					elemContainer.innerHTML = htmlString;
				};

				var addEvent = function(target , eventName , fn){
					var i = 0;
					if(target.length){
						for(; i < target.length; i++){
							target[0].addEventListener(eventName , fn);
						}
					}else{
						target.addEventListener(eventName , fn);
					}
				};


				var removeChild = function(target , child){
					var i = 0;
					if(!child || child.length == 0){
						return;
					}

					if(child.length){
						for(; i < child.length; i++){
							target.removeChild(child[i]);
						}
					}else{
						target.removeChild(child);
					}
				}

				$('.customise_main .masthead .start_miadi').bind('click',function(){
					var offsetTop = $('.mi_products').offset().top;

					$('html,body').animate({
						scrollTop : offsetTop
					} , 600);
				});

				$('.mi_products_tab a').bind('click',function(){
					if($(this).hasClass('on')){
						return;
					}

					var ctgrArr = ['running' , 'originals' , 'basketball'];

					$('.mi_products_tab a').removeClass('on');
					$(this).addClass('on');

					viewCtgr = ctgrArr[$(this).index()];

					renderMiProducts(miProducts[viewCtgr] , viewItemLength);
				});

				renderMiProducts(miProducts[viewCtgr] , viewItemLength);

			})(jQuery);

