	<script type="text/javascript">

		(function($){

			setTimeout(function(){
				$('.main').addClass('on');
			},500)

			var owlSaleProducts = $("#product-list").owlCarousel({
				stagePadding: 80,
				margin:5,
				items : 1,
				merge: true
			});
			owlSaleProducts.on('changed.owl.carousel', function(event) {
				var index = Math.floor(event.item.index/2);
				if(event.item.count%2 !== 0 && event.item.index == event.item.count - 2){
					$(event.target).find('.owl-controls .owl-dot').removeClass('active').last().addClass('active');
				}
			});


			var scrollArray = [];
			// var thisPos = $(this).scrollTop();
			var scrollCon = document.querySelectorAll('.scrollbox');

			for(var i = 0; i < scrollCon.length; i++){
				var mypos = scrollCon[i].offsetTop;
				scrollArray.push(mypos);
			}



			$(window).scroll(function(){
				for(var pos = 0; pos < scrollArray.length; pos++){
					var thisPos = $(this).scrollTop() + $('.main').height();
					if(thisPos >= scrollArray[pos]){
						$('.scrollbox').eq(pos).addClass('on');
					}
				}
			});


		})(jQuery);

	</script>
