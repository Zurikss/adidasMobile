		<script type="text/javascript">
			function dataSend(){
				//이름 체크
				if(jQuery("#field1").val() == ""){
					alert("이름을 입력하세요.");
					jQuery("#field1").focus();
					return;
				}

				//성별 체크
				if(jQuery("#field2").val()== ""){
					alert("성별을 선택하세요.");
					jQuery("#field2").focus();
					return;
				}

				//E-Mail 체크
				if(jQuery("#field0").val() == ""){
					alert("E-Mail을 입력하세요.");
					jQuery("#field0").focus();
					return;
				} else {
					if(!checkEmail()){
						alert("E-Mail 형식을 확인 하세요.");
						jQuery("#field0").focus();
						return;
					}
				}

				//마케팅 수신 동의 체크
				if(document.getElementById('mark_chk').checked == false) {
					alert("마케팅 수신 동의를 체크해 주세요.");
					jQuery("#mark_chk").focus();
					return;
				}
				document.getElementById('form45').submit();
			}

			//이메일 형식 체크
			function checkEmail(){
				var emailVal = jQuery("#field0").val();
				var reg = /^((\w|[\-\.])+)@((\w|[\-\.][^(\.)\1])+)\.([A-Za-z]+)$/;
				if (emailVal.search(reg) != -1 && emailVal.length < 31) {
					return true;
				}
				return false;
			}
