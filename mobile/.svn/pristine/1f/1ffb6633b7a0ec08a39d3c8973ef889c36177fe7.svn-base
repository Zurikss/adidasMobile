	<script type="text/javascript">


		(function($){
				
					var video_offset = $('.video_wrap').offset().top;
					console.log(video_offset)

					//스크롤이벤트
					var scrollArray = [];
					var scrollContent = document.querySelectorAll('.scroll');
					for(var i = 0; i<scrollContent.length; i++){
						 var mypos = scrollContent[i].offsetTop;
						 scrollArray.push(mypos)
					}

					$(window).scroll(function(){
						var thispos = $(this).scrollTop() + 100;
						for(var pos = 0; pos<scrollArray.length; pos++){
							if(thispos>scrollArray[pos]){
								$('.pod_system .scroll').eq(pos).addClass('on');
							}
						}
					});

				


				// bottom slider
				var owlSlider2 = $('#slider_owl2');

				owlSlider2.on('initialized.owl.carousel', function(event) {
					/*
						최초 로딩 시 한번만 호출하는 함수
					*/
				});

				owlSlider2.on('changed.owl.carousel', function(event) {
					/*
						Callback 이벤트 (슬라이드가 실행 될 떄마다 실행되는 함수)
						* index 사용하기
						event.item.index

						* length 사용하기
						event.item.count
					*/
				});

				owlSlider2.owlCarousel({
					stagePadding: 0,
					margin:0,
					items : 1,
					merge: true,
					loop : true,
					navigation: true,
					nav: false,
					autoplay:true,

				});

				//셋타임아웃
				setTimeout(function(){
					$('.pod_system .box1 .shoes').addClass('on');
				},300);





				// middle slider
				var owlSlider = $('#slider_owl');

				owlSlider.on('initialized.owl.carousel', function(event) {
					/*
						최초 로딩 시 한번만 호출하는 함수
					*/
				});

				owlSlider.on('changed.owl.carousel', function(event) {
					var index = event.item.index;
					$('#slider_owl .owl-item li').removeClass('on').eq(index).addClass('on');
