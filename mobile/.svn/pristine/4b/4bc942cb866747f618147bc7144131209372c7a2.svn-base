.zipcode #zipcode_wrap2 .zip_box2 .box1 select{width:30%;float:left;font-weight:normal;font-size:12px;letter-spacing:-1px;}
.zipcode #zipcode_wrap2 .zip_box2 .box2{height:18px;position:relative;margin-top:10px;}
.zipcode #zipcode_wrap2 .zip_box2 .box2 select{
	width:22%;
	margin-right:3%;
	position:absolute;
	height:19px;
	top:1px;
	left:0;
	float:left;
	font-weight:normal;
	font-size:12px;
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;	
}
.zipcode #zipcode_wrap2 .zip_box2 .box2 .ipt_box{position:relative;margin:0 45px 0 25%;}
.zipcode #zipcode_wrap2 .zip_box2 .box2 input{width:100%;height:17px;margin-top:3px;font-weight:normal;font-size:12px;}
.zipcode #zipcode_wrap2 .zip_box2 .box2 .ipt_box select{
	width:100%;height:19px;margin-top:0;font-weight:normal;font-size:12px;
