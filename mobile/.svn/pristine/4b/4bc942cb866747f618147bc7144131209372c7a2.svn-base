#contents .adi_blog{padding:25px 10px 0;color:#000;letter-spacing:-1px;}
#contents .adi_blog h3{color:#636363;font-size:14px;}
#contents .adi_blog li{padding:15px 0;border-bottom:1px solid #dbdbdb;}
#contents .adi_blog li dt{margin-bottom:10px;color:#111;font-size:13px;}
#contents .adi_blog li dd{overflow:hidden;position:relative;} 
#contents .adi_blog li dd .img{max-width:115px;width:30%;float:left;}
#contents .adi_blog li dd .img img{width:100%;}
#contents .adi_blog li dd .info{
	width:70%;
	float:left;
	padding-left:10px;
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
}
#contents .adi_blog li dd .info .desc{margin-bottom:6px;max-height:46px;overflow:hidden;color:#555;font-size:11px;font-weight:normal;line-height:13px;}
#contents .adi_blog li a.more{clear:both;font-size:9px;letter-spacing:0;}
#contents .adi_blog .more_view{
   background: none repeat scroll 0 0 #fff;
    color: #333;
    display: block;
    height: 75px;
    line-height: 75px;
    text-align: center;
    width: 100%;
}
#contents .adi_blog .more_view.loading img{
	animation:camp_loading 900ms linear 100ms infinite;
	-webkit-animation:camp_loading 900ms linear 100ms infinite;
	-moz-animation:camp_loading 900ms linear 100ms infinite;
}
#contents .adi_blog .more_view img{width: 18px;margin-right:6px;vertical-align: middle;}


#footer_r .bottom_menu{height:85px}
#footer_r .bottom_menu li{width:25%;height:85px;float:left;}
#footer_r .bottom_menu li a{
	height:85px;
	display:block;
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	text-align:center;
	border-right:1px solid #444;
	border-left:1px solid #000;
	color:#fff;
	background:#222;
	font-size:13px;
}
#footer_r .bottom_menu li a span{margin-top:57px;}
#footer_r .bottom_menu li:nth-child(1) a{
	border-left-width:0;
	background:#222 url(../../images/adidas/bg_btm_menu1.png) 50% 18px no-repeat;
	background-size:36px auto;
}
#footer_r .bottom_menu li:nth-child(2) a{
	background:#222 url(../../images/adidas/bg_btm_menu2.png) 50% 16px no-repeat;
	background-size:35px auto;
}
#footer_r .bottom_menu li:nth-child(3) a{
	background:#222 url(../../images/adidas/bg_btm_menu3.png) 50% 20px no-repeat;
	background-size:33px auto;
}
#footer_r .bottom_menu li:nth-child(4) a{
	border-right-width:0;
	background:#222 url(../../images/adidas/bg_btm_menu4.png) 50% 16px no-repeat;
	background-size:31px auto;
}

#container .promo_list li a{display:block;}
#container .promo_list li a img{width:100%;}

#dqAutoTop{}
#dqAutoTop ul.search_list{width:100%;background:#fff;border-bottom:#eee;box-shadow:0px 0px 8px #000;}
#dqAutoTop ul.search_list li{height:30px;}
#dqAutoTop ul.search_list li a{display:block;height:30px;padding:0 10px;line-height:30px;font-size:13px;}
#dqAutoTop p.close{display:none;}

.zipcode{}
.zipcode h2{margin-bottom:15px;padding:15px 0 0 10px;letter-spacing:-1px;font-size:19px;font-weight:normal;color:#4d4d4d;text-shadow:1px 1px 0 #a5a5a5;}
.zipcode .tab{height:32px;margin-bottom:10px;}
.zipcode .tab li{
	width:50%;
	height:30px;
	float:left;
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
}
.zipcode .tab li a{
	display:block;
	width:100%;
	height:24px;
	padding-top:6px;
	border:1px solid #ddd;
	text-align:center;
	font-size:13px;
	color:#939393;
	background:#F7F7F7;
}
.zipcode .tab li:first-child a{border-right-width:0px;}
.zipcode .tab li:last-child a{border-width:1px 0 1px 1px;}
.zipcode .tab li a.on{border-bottom-color:#fff;color:#000;background:#fff;}
.zipcode #zipcode_wrap2 dl{margin:25px 0;padding-left:12px;}
.zipcode #zipcode_wrap2 dt{margin-bottom:10px;font-size:12px;color:#000;}
.zipcode #zipcode_wrap2 dd li{margin-bottom:2px;font-size:12px;color:#666;font-weight:normal;}
.zipcode #zipcode_wrap2 dd li strong{color:#000;font-weight:bold;}
.zipcode #zipcode_wrap2 .zip_box2{overflow:hidden;margin-bottom:20px;padding:15px 15px 20px;border-top:1px solid #d9d9d9;border-bottom:1px solid #d9d9d9;background:#f6f6f6;}
.zipcode #zipcode_wrap2 .zip_box2 p{margin-bottom:15px;color:#444;font-size:12px;}
