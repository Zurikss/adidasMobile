.card_info_pop{margin:0 10px;position:absolute;background:#fff;}
.card_info_pop .top{height:28px;padding:12px 10px 0;position:relative;background:#4e6cce;}
.card_info_pop .top a{width:40px;height:40px;display:block;position:absolute;top:0;right:0;}
.card_info_pop .top a:after{
	content: '';
	width: 1px;
	height: 34px;
	-moz-transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	transform: rotate(45deg);
	position: absolute;
	top: 3px;
	right: 19px;
	background: #aaa;	
}
.card_info_pop .top a:before{
	content: '';
	width: 1px;
	height:34px;
	-moz-transform: rotate(-45deg);
	-webkit-transform: rotate(-45deg);
	transform: rotate(-45deg);
	position: absolute;
	top: 3px;
	right:19px;
	background: #aaa;
}
.card_info_pop .top h2{color:#fff;font-size:13px;}
.card_info_pop .body{border:2px solid #4e6cce;}
.card_info_pop .body h3{padding:10px;font-size:12px;background:#eceef4;color:#383838;}
.card_info_pop .body .box{padding-bottom:10px;border-bottom:1px solid #4e6cce;}
.card_info_pop .body table{width:100%;table-layout:fixed;}
.card_info_pop .body th{padding:5px 3px;text-align:left;color:#383838;font-weight:normal;}
.card_info_pop .body td{padding:3px 3px;color:#383838;font-weight:normal;}
.card_info_pop .body .info{padding:5px 10px;border-bottom:1px dotted #b2b2b2;}
.card_info_pop .body .desc{padding:5px 10px 0;}
.card_info_pop .body .desc li{margin-bottom:3px;padding-left:8px;line-height:14px;color:#999;font-size:11px;position:relative;font-weight:normal;}
.card_info_pop .body .desc li:before{content: '-';	position: absolute;top:0;left:0;}