.zipcode #zipcode_wrap2 .zip_box2 .box2 a{position:absolute;top:-3px;right:0;}
.zipcode #zipcode_wrap2 .zip_box2 .inbtn{margin-top:25px;padding-top:20px;border-top:1px solid #d9d9d9;text-align:center;}
.zipcode #zipcode_wrap2 .zip_box2 .zip_list{margin-bottom: 0;margin-top:5px;}
.zipcode #zipcode_wrap2 .zip_box2 .zip_list li:last-child{border-bottom-width:0px;}

