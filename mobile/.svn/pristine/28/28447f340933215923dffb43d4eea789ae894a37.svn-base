    <script type="text/javascript">

        (function($){

            var varUA = navigator.userAgent.toLowerCase();
            //userAgent 값 얻기

            if (varUA.match('android') != null) {
                //안드로이드 일때 처리

                var maintop = document.getElementById('customise_top');

                maintop.innerHTML= '<img src="/images/adidas/event/alphabounce/main.jpg" alt="">';

            } else if (varUA.indexOf("iphone")>-1||varUA.indexOf("ipad")>-1||varUA.indexOf("ipod")>-1) {
                //IOS 일때 처리

            } else {
                //아이폰, 안드로이드 외 처리
            }

            var scrollF = function(){

                var scrollArray = [];
                // var thisPos = $(this).scrollTop();
                var scrollCon = document.querySelectorAll('.scrollbox');

                for(var i = 0; i < scrollCon.length; i++){
                    var mypos = scrollCon[i].offsetTop;
                    scrollArray.push(mypos);
                }

                for(var pos = 0; pos < scrollArray.length; pos++){
                    var thisPos = $(this).scrollTop() ;
                    if(thisPos >= scrollArray[pos]){
                        $('.scrollbox').eq(pos).addClass('on');
                    }
                }
            }

            $(window).scroll(scrollF);


        })(jQuery);

    </script>



