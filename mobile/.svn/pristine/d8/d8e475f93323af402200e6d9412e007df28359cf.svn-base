					var thisPos = $(this).scrollTop() + $(window).height() - 50;
					// console.log(thisPos);

					for (var pos = 0; pos < scrollArray.length; pos++) {
						if (thisPos > scrollArray[pos]) {

							$('#falcon .scrollevent').eq(pos).addClass('on');
						}
					}
				})
			})

		})(jQuery);

	</script>
