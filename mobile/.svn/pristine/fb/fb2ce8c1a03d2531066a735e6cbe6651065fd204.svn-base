	<script type="text/javascript">
		(function($){


			var collect2 = $('.collections2').find('li');
			var setUrl;

			// 하단 신발 구매리스트
			collect2.on('click', function () {
				clearTimeout(setUrl);

				var urlId = $(this).attr('data-url-id');

				$(this).siblings().find('.box_effect, .btn-now').removeClass('on');
				$(this).find('.box_effect, .btn-now').addClass('on');

				setUrl = setTimeout(function () {
					window.location.href = urlId;
				}, 2400);
			});

			$('.video-btn-id').bind('click',function(){
				//alert('동영상이 준비중입니다');
				var ytbId = $(this).attr('data-video-id');
				var html = '<div id="video-layer" style="">';
				html += '	<div class="bg" ></div>';
				html += '	<div class="video-box"></div>';
				html += '	<a href="javascript:void(0)" class="close_x_btn white"></a>';
				html += '</div>';

				$('html').css({overflow: 'hidden'});
				$('body').append(html);

				$('#video-layer .close_x_btn').bind('click',function(){
					$('html').css({overflow: 'scroll'});
					$('#video-layer ').remove();
				});


				setTimeout(function(){
					$('#video-layer').addClass('loaded');

					$('#video-layer .video-box').append('<iframe width="100%" height="100%" src="http://www.youtube.com/embed/'+ytbId+'?rel=0" frameborder="0" allowfullscreen></iframe>')
				},100);
			});

			var tag = document.createElement('script');

				tag.src = "https://www.youtube.com/iframe_api";
				var firstScriptTag = document.getElementsByTagName('script')[0];
				firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

				var player;
				function loadVideo(videoId,targetId) {
				player = new YT.Player(targetId, {
						height: '100%',
						width: '100%',
						videoId: videoId,
						playerVars: {
							controls: 0,
							showinfo : 0,
							autohide : 0,
							disablekb : 0,
							loop : 1,
							modestbranding : 1,
							rel : 0
						}
				});

				player.playVideo();
				}

				// 4. The API will call this function when the video player is ready.
				function onPlayerReady(event) {
				event.target.playVideo();
			}

			// 5. The API calls this function when the player's state changes.
			//    The function indicates that when playing a video (state=1),
			//    the player should play for six seconds and then stop.
			var done = false;
			function onPlayerStateChange(event) {
				if (event.data == YT.PlayerState.PLAYING && !done) {
				  setTimeout(stopVideo, 6000);
				  done = true;
				}
			}
			function stopVideo() {
				player.stopVideo();
			}

			// 1 하단 갤러리
			var productsSlider1 = Swipe(document.getElementById('collection_slider1'), {
				stopPropagation: true,

				callback: function (index, element) {
					$('.collection_slider1 .num_paging a').removeClass('on').eq(index).addClass('on');
					$('.collection_slider1 .prodlist_wrap .prodlist').removeClass('active').eq(index).addClass('active');
				}
			});

			// 1 하단 갤러리넘버링

			$('.collection_slider1 .num_paging a').bind('click', function () {
				if ($(this).hasClass('on')) {
					return;
				}
				productsSlider1.slide($(this).index());
			});

			$('.collection_slider1 .next').click(function () {
				productsSlider1.next();
				return false;
			});
			$('.collection_slider1 .prev').click(function () {
				productsSlider1.prev();
				return false;
			});

			// 2 하단 갤러리
			var productsSlider2 = Swipe(document.getElementById('collection_slider2'),{
				stopPropagation: true,

				callback: function(index, element) {
					$('.collection_slider2 .num_paging a').removeClass('on').eq(index).addClass('on');
					$('.collection_slider2 .prodlist_wrap .prodlist').removeClass('active').eq(index).addClass('active');
				}
			});

			// 2 하단 갤러리넘버링

			$('.collection_slider2 .num_paging a').bind('click',function(){
				if($(this).hasClass('on')){
					return;
				}
				productsSlider2.slide($(this).index());
			});

			$('.collection_slider2 .next').click(function(){
				productsSlider2.next();
				return false;
			});
			$('.collection_slider2 .prev').click(function(){
				productsSlider2.prev();
				return false;
			});

			var scrollArray = [];
			var scrollContents = document.querySelectorAll('.scroll');
			for (var i = 0; i < scrollContents.length; i++) {
				var mypos = scrollContents[i].offsetTop;
				scrollArray.push(mypos)
			}
			$(window).scroll(function () {
				var thisPos = $(this).scrollTop() + 300;
				for (var pos = 0; pos < scrollArray.length; pos++) {
					if (thisPos > scrollArray[pos]) {
						$('.scroll').eq(pos).addClass('on');
					}
				}
			});

		})(jQuery);

	</script>

