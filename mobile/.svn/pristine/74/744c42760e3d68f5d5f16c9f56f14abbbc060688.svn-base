					if($(this).find('a').hasClass('on')){
						return false;
					}

					if($(this).hasClass('type_box')){
						$('.product_grid').addClass('grid_2box');
					}else{
						$('.product_grid').removeClass('grid_2box');
					}

					$('.toolbox>ul>li>a').removeClass('on');
					$(this).find('a').addClass('on');
					return false;

				});

			});
		})(jQuery);

