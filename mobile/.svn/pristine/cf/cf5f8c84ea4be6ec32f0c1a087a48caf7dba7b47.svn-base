			<script type="text/javascript">

				(function($){

                    var winW = $(window).width(),
						winH = $(window).height();

					var gateIndi = $('.gate_indi_wrap').find('button'),
						gateLink = $('.gate_slide_wrap').find('a'),
						popClose = $('.pop_close_btn');

					var popWrap = $('.pop_slide'),
						popImgWrap = $('.pop_img_wrap'),
						popNumWrap = $('.pop_num_wrap'),
						popIndiWrap = $('.pop_indi_wrap');

					/* window width에 따른 값 변경 이벤트 */
					$(document).ready(function(){

						loadWindow(winW, winH);
					});

					$(window).resize(function(){

						var winRW = $(window).width(),
							winRH = $(window).height();

						if(winRW > winW){
							resizeWindow(winRW, winRH, winW);
						}else{
							resizeWindow(winRW, winRH, winW);
						}
					});

					// 게이트 이동시 인디게이트 활성화
					$('.gate_slide_wrap').on('touchmove', function(){

						var thisL = $(this).scrollLeft(),
							thisC = $(this).find('li').width();

						if(0 < thisL && (thisC - 100) > thisL){

							$('.gate_indi_wrap').find('li').removeClass('act_indi');
							$('.gate_indi_wrap').find('li').eq(0).addClass('act_indi');
						}else if((thisC - 100) <= thisL && ((thisC * 2) - 100) > thisL){

							$('.gate_indi_wrap').find('li').removeClass('act_indi');
							$('.gate_indi_wrap').find('li').eq(1).addClass('act_indi');
						}else if((thisC * 2 - 100) <= thisL ){

							$('.gate_indi_wrap').find('li').removeClass('act_indi');
							$('.gate_indi_wrap').find('li').eq(2).addClass('act_indi');
						}
					})

					// 게이트 인디게이트 클릭시
					gateIndi.on('click', function(){

						var thisI = $(this).parent('li').index(),
							windowW = $(window).width();

						if(windowW >= 571){
							gate_indi(thisI, 570);
						}else if(windowW >= 441 && windowW < 571){
							gate_indi(thisI, 380);
						}else if(windowW >= 361 && windowW < 441){
							gate_indi(thisI, 285);
						}else{
							gate_indi(thisI, 255);
						}
					});

					// 게이트 클릭시
					gateLink.on('click', function(){

						var thisI = $(this).parent('li').index(),
							windowW = $(window).width(),
							windowH = $(window).height();

						if($('.gate_slide_wrap').hasClass('load_row')){
							if(windowW >= 361){
								gate_enter_row(windowW, windowH, thisI, 12, 8);
							}else{
								gate_enter_row(windowW, windowH, thisI, 10, 5);
							}
						}else{
							if(windowW >= 441){
								gate_enter_col(windowW, thisI, 15, 10);
							}else if(windowW >= 361 && windowW < 441){
								gate_enter_col(windowW, thisI, 12, 8);
							}else{
								gate_enter_col(windowW, thisI, 10, 5);
							}
						}

					});

					/* 공통사용 이벤트(window.width 상관 없이) */
					// popup창 close 클릭 시
					popClose.on('click', function(){

						popImgWrap.fadeOut(400);
						popWrap.delay(300).fadeOut(300, function(){
							popImgWrap.css({marginLeft: 0});
						});

						popNumWrap.find('p').remove();
						popIndiWrap.find('li').remove();

						popWrap.removeClass('popupActive');
						popImgWrap.removeClass('activeImg');

						var widW = $(window).width(),
							widH = $(window).height();

						loadWindow(widW, widH);
					});

					// popup창 내 인디게이트 클릭 시
					popIndiWrap.on('click', '.img_indi', function(){

						var thisI = $(this).parent('li').index(),
							winWidth = $(window).width(),
							imgLeft = thisI * winWidth;

						popImgWrap.animate({marginLeft: -imgLeft}, 500);

						popNumWrap.find('p').stop().animate({opacity: 0}, 200);
						popNumWrap.find('p').eq(thisI).stop().animate({opacity: 1}, 400);

						popIndiWrap.find('li').removeClass('act_img_indi');
						popIndiWrap.find('li').eq(thisI).addClass('act_img_indi');
					});

					/* 함수 모음 */
					// 메인 인디게이트 설정 함수
					function gate_indi(index, li_width){

						var gatestyleL = $('.gate_slide_wrap').find('ul').css('padding-left'),
							gateL = gatestyleL.substring(0, 2);

						if(index === 0){
							$('.gate_slide_wrap').animate({scrollLeft : 0}, 500);
						}else if(index === 1){
							$('.gate_slide_wrap').animate({scrollLeft : li_width - gateL}, 500);
						}else{
							$('.gate_slide_wrap').animate({scrollLeft : li_width * 2}, 500);
						}

						$('.gate_indi_wrap').find('li').removeClass('act_indi');
						$('.gate_indi_wrap').find('li').eq(index).addClass('act_indi');
					}

					// popup창 로드시 세로 화면일 경우
					function gate_enter_col(winWid, index, indiW, indiMR){

						var activeImgWrap = $('#gate0' + (index+1)),
							activeImgL = activeImgWrap.find('li').length,
							imgW = winWid * activeImgL,
							imgH = Math.abs((300 * winWid ) / 533),
							shopLinkH = activeImgWrap.find('a').height();

						activeImgWrap.css({width: imgW, marginTop: -imgH / 2});
						activeImgWrap.addClass('activeImg')
						activeImgWrap.find('li').width(winWid);
						activeImgWrap.find('a').css({marginTop: imgH - shopLinkH - 20 });

						for(i=0; i<activeImgL; i++){
							if (i < 9) {
								popNumWrap.append('<p> 0'+ (i + 1) +'</prom>');
							} else {
								popNumWrap.append('<p>'+ (i + 1) +'</prom>');
							}
							popIndiWrap.append('<li><a href="#!" class="img_indi">' + (i + 1) + '</a></li>');
						}

						popNumWrap.find('p').eq(0).css({opacity: 1});

						var indiele = indiW + indiMR,
						indiWrapW = ((activeImgL - 1) * indiele) + indiW ;

						popIndiWrap.css({width: indiWrapW, marginLeft: (-indiWrapW / 2)});
						popIndiWrap.find('li').css({width : indiW, height: indiW, marginRight: indiMR });
						popIndiWrap.find('li').eq(activeImgL - 1).css({marginRight: 0});
						popIndiWrap.find('li').eq(0).addClass('act_img_indi');

						popWrap.fadeIn(300);
						activeImgWrap.delay(200).fadeIn(400);

						popWrap.addClass('popupActive');
					}

					// popup창 로드시 가로 화면일 경우
					function gate_enter_row(winWid, winHei, index, indiW, indiMR){

						var activeImgWrap = $('#gate0' + (index+1)),
							activeImgL = activeImgWrap.find('li').length,
							imgW = winWid * activeImgL,
							imgH = Math.abs(winHei / 2),
							shopLinkH = activeImgWrap.find('a').height();

						activeImgWrap.css({width: imgW, marginTop: -imgH});
						activeImgWrap.addClass('activeImg')
						activeImgWrap.find('li').width(winWid);
						activeImgWrap.find('a').css({marginTop: winHei - shopLinkH - 20 });

						for(i=0; i<activeImgL; i++){

							popIndiWrap.append('<li><a href="#!" class="img_indi">' + (i + 1) + '</a></li>');
						}

						var indiele = indiW + indiMR,
							indiWrapW = ((activeImgL - 1) * indiele) + indiW ;

						popIndiWrap.css({width: indiWrapW, marginLeft: (-indiWrapW / 2), top: '100%', marginTop: -50});
						popIndiWrap.find('li').css({width : indiW, height: indiW, marginRight: indiMR });
						popIndiWrap.find('li').eq(activeImgL - 1).css({marginRight: 0});
						popIndiWrap.find('li').eq(0).addClass('act_img_indi');

						popWrap.fadeIn(300);
						activeImgWrap.delay(200).fadeIn(400);

						popWrap.addClass('popupActive');
					}

					function resize_pop_col(winR, winH){

						console.log('resize_pop_col 로드');

						var activeImgL = $('.activeImg').find('li').length,
							imgW = winR * activeImgL,
							imgH = Math.abs(winH / 2),
							imgL = popIndiWrap.find('.act_img_indi').index(),
							shopLinkH = $('.activeImg').find('a').height();

						$('.activeImg').css({width: imgW, marginTop: -imgH, marginLeft: -winR * imgL});
						$('.activeImg').find('li').css({width : winR, overflow: 'hidden'});
						$('.activeImg').find('a').css({marginTop: winH - shopLinkH - 20 });

						popNumWrap.hide();

						imgIndiStyle_bottom(activeImgL, winR);

						popWrap.removeClass('popupActive');
						popWrap.addClass('resize');
					}

					function resize_pop_row(winR){

						console.log('resize_pop_row 로드');

						var activeImgL = $('.activeImg').find('li').length,
							imgW = winR * activeImgL,
							imgH = Math.abs((300 * winR ) / 533),
							imgL = popIndiWrap.find('.act_img_indi').index(),
							shopLinkH = $('.activeImg').find('a').height();

						$('.activeImg').css({width: imgW, marginTop: -imgH / 2, marginLeft: -winR * imgL});
						$('.activeImg').find('li').css({width : winR, overflow: 'hidden'});
						$('.activeImg').find('a').css({marginTop: imgH - shopLinkH - 20 });

						for(i=0; i<activeImgL; i++){
							if (i < 9) {
								popNumWrap.append('<p> 0'+ (i + 1) +'</prom>');
							} else {
								popNumWrap.append('<p>'+ (i + 1) +'</prom>');
							}
						}

						popNumWrap.show();
						popNumWrap.find('p').eq(imgL).css({opacity: 1});

						imgIndiStyle_center(activeImgL, winR);

						popWrap.removeClass('popupActive');
						popWrap.addClass('resize');
					}

					function back_pop_col(winW){

						console.log('back_pop_col 로드');

						var activeImgL = $('.activeImg').find('li').length,
							imgW = winW * activeImgL,
							imgH = Math.abs((300 * winW ) / 533),
							imgL = popIndiWrap.find('.act_img_indi').index(),
							shopLinkH = $('.activeImg').find('a').height();

						$('.activeImg').css({width: imgW, marginTop: -imgH / 2, marginLeft: -winW * imgL});
						$('.activeImg').find('li').width(winW);
						$('.activeImg').find('a').css({marginTop: imgH - shopLinkH - 20 });

						popNumWrap.show();

						imgIndiStyle_center(activeImgL, winW);

						popWrap.removeClass('resize');
						popWrap.addClass('popupActive');
					}

					function back_pop_row(winRW, winH){

						console.log('back_pop_row 로드');

						var activeImgL = $('.activeImg').find('li').length,
							imgW = winRW * activeImgL,
							imgH = Math.abs(winH / 2),
							imgL = popIndiWrap.find('.act_img_indi').index(),
							shopLinkH = $('.activeImg').find('a').height();

						$('.activeImg').css({width: imgW, marginTop: -imgH, marginLeft: -winRW * imgL});
						$('.activeImg').find('li').width(winRW);
						$('.activeImg').find('a').css({marginTop: winH - shopLinkH - 20 });

						popNumWrap.hide();

						imgIndiStyle_bottom(activeImgL, winRW);

						popWrap.removeClass('resize');
						popWrap.addClass('popupActive');
					}

					function imgIndiStyle_center(actL, winWidth){

						console.log('imgIndiStyle_center 실행');

						var indiW = 0,
							indiMR = 0,
							indiWrapW = 0;

						if(winWidth >= 441){
							indiW = 15;
							indiMR = 10;
						}else if(winWidth >= 361 && winWidth < 441){
							indiW = 12;
							indiMR = 8;
						}else{
							indiW = 10;
							indiMR = 5;
						}

						var indiele = indiW + indiMR,
							indiWrapW = ((actL - 1) * indiele) + indiW ;

						popIndiWrap.css({width:indiWrapW, marginLeft: (-indiWrapW / 2), top: '50%', marginTop: "38%"});
						popIndiWrap.find('li').css({width : indiW, height: indiW, marginRight: indiMR });
						popIndiWrap.find('li').eq(actL - 1).css({marginRight: 0});
					}

					function imgIndiStyle_bottom(actL, winWidth){

						console.log('imgIndiStyle_bottom 실행');

						var indiW = 0,
							indiMR = 0,
							indiWrapW = 0;

						if(winWidth >= 361){
							indiW = 12;
							indiMR = 8;
						}else{
							indiW = 10;
							indiMR = 5;
						}

						var indiele = indiW + indiMR,
							indiWrapW = ((actL - 1) * indiele) + indiW ;

						popIndiWrap.css({width:indiWrapW, marginLeft: (-indiWrapW / 2), top: '100%', marginTop: -50});
						popIndiWrap.find('li').css({width : indiW, height: indiW, marginRight: indiMR });
						popIndiWrap.find('li').eq(actL - 1).css({marginRight: 0});
					}

					function resizeWindow(winRW, winRH, winW){

						if($('.gate_slide_wrap').hasClass('load_row')){
							if(popWrap.hasClass('popupActive')){
								resize_pop_row(winRW);
							}else if(popWrap.hasClass('resize')){
								back_pop_row(winRW, winH);
							}else{
								loadWindow(winRW, winRH);
							}
						}else if($('.gate_slide_wrap').hasClass('load_col')){
							if(popWrap.hasClass('popupActive')){
								resize_pop_col(winRW, winRH);
							}else if(popWrap.hasClass('resize')){
								back_pop_col(winRW);
							}else{
								loadWindow(winRW, winRH);
							}
						}
					}

					function loadWindow(winLW, winLH){

						if(winLW > winLH){
							/* 모바일이 가로로 누워 있을 경우 */
							$('.gate_slide_wrap').removeClass('load_col');
							$('.gate_slide_wrap').addClass('load_row');
						}else{
							/* 모바일이 세로로 서 있을 경우 */
							$('.gate_slide_wrap').removeClass('load_row');
							$('.gate_slide_wrap').addClass('load_col');

						}
					}

					// video
                    $('.video-button').bind('click', function() {
                        var ytbId = $(this).attr('data-video-id');
                        var html = '<div id="video-layer" style="">';
                        html += '	<div class="bg" ></div>';
