	<script type="text/javascript">
		(function($){

			$('.sorting_wrapper .switch_view a').bind('click',function(){
				var gridName;
				$('.sorting_wrapper .switch_view a').each(function(){
					if($(this).hasClass('on')){
						$(this).removeClass('on')
					}else{
						gridName = this.dataset.gridname;
						$(this).addClass('on');
					}
				});

				if(gridName == 'list'){
					$('.product_grid').removeClass('grid_2box');
				}else{
					$('.product_grid').addClass('grid_2box');
				}

			});


			$('.filter_wrapper ul>li').bind('click',function(){
				$('.filter_container').css({
					width : $(window).width() - 60
				});
				$('.filter_container_wraper').addClass('open');
				if(this.dataset.ctgr !== undefined){
					$('.category_box>.box.category_'+this.dataset.ctgr).addClass('open');
				}
			});

			$('.filter_container_wraper>.close_x_btn , .filter_container_wraper>.overlay').bind('click',function(){
				$('.filter_container_wraper').removeClass('open');
				$('.category_box>.box').removeClass('open');
			});

			$('.filter_container_wraper>.overlay').bind('touchmove',function(event){
				event.preventDefault();
			});

			$('.filter_container_wraper .category_box .box>.tit').bind('click',function(){
				var elemParent = $(this).parent();
				if(elemParent.hasClass('open')){
					elemParent.removeClass('open');
				}else{
					elemParent.addClass('open');
					$('.filter_container_wraper .filter_container').animate({
						scrollTop : elemParent.offset().top
					})
				}
			});

			$('.filter_container .category_products ul li').bind('click',function(event){
				event.stopPropagation();

				if($(this).hasClass('open')){
					$(this).removeClass('open');
					$(this).children('a').removeClass('open');
				}else{
					$(this).addClass('open');
					$(this).children('a').addClass('open');
				}
			});

		})(jQuery);

	</script>

