@import './app/css/adidas/scss/common/mixin.scss';
@import './app/css/adidas/scss/common/common.scss';

$break-small: 1000px;
$break-large: 1400px;
$width320:320px;
$width400:400px;
$width480:480px;
$width600:600px;

/* customise_main */
.customise_main{
	width: 100%;
	position: relative;
	overflow: hidden;


	/*
	 * Common use
	   button
	 */
	.button{
		width: 340px;
		height: 62px;
		display: block;
		font:400 16px/58px 'NotoSansKR';
		box-sizing: border-box;
		-webkit-box-sizing: border-box;
		color: #000;
		text-decoration: none;
		letter-spacing:2px;
		border: 1px solid  #000;
		text-align: center;

		@include css3(transition,all 0.4s ease 0.1s);

		.arrow{
			margin-left: 25px;
			position: relative;
			display: inline-block;
			width: 40px;
			height: 10px;
			background:url('http://image.adidas.co.kr/images/adidas/event/customise/miadidas-interface.png') -13px -45px;
			opacity: 1;
			@include css3(transition,opacity 0.4s ease 0.1s);
		}

		.over_arrow{
			display: inline-block;
			position: absolute;
			top: 0;
			left: 0;
			width: 40px;
			height: 10px;
			background:url('http://image.adidas.co.kr/images/adidas/event/customise/miadidas-interface.png') -55px -35px;
			opacity: 0;
			@include css3(transition,opacity 0.4s ease 0.1s);
		}

		&.black{
			background: #000;
			color: #fff;

			.arrow{
				background-position: -55px -35px;
			}

			.over_arrow{
				background-position: -13px -45px;
			}

			&:hover{
				background: #fff;
				color:#000;
			}
		}

		&:hover{
			background: #000;
			color: #fff;

			.over_arrow{
				opacity: 1;
			}
		}
	}
	/* button END */

	.mi-button{
		width: 200px;
		position: relative;
		display: inline-block;
		overflow: hidden;
		padding: 15px 0;
		background: #000;
		text-align: center;
		line-height: 1.3rem;

		span.text{
			position: relative;
			color: #fff;
			font-size: 1.3rem;
			z-index: 5;
			@include css3(transition,color 0.2s ease 0.08s);
		}

		.arrow{
			width: 35px;
			height: 1px;
			position: relative;
			margin-left: 8px;
			display: inline-block;
			vertical-align: middle;
			background: #fff;

			&::before , &::after{
				content:'';
				display: block;
				width: 10px;
				height: 1px;
				position: absolute;
				top: 0;
				right: 0;
				background:#fff;
				@include css3(transform-origin,top right);
				@include css3(transform , rotate(45deg));
			}

			&::after{
				right: 1px;
				@include css3(transform , rotate(-45deg));
			}
		}

		&::after{
			content:'';
			display: block;
			position: absolute;
			top: 0;
			left: -100%;
			width: 100%;
			height: 100%;
			background: #fff;

			@include css3(transition,left 0 cubic-bezier(0.46, 0.03, 0.52, 0.96));
		}

		&.hover{
			&::after{
				left: 0;
				@include css3(transition-duration,0.25s);
			}

			span.text{
				color: #000;
			}
		}

		&.out{
			&::after{
				left: 100%;
				@include css3(transition-duration,0.25s);
			}

			span.text{
				color: #fff;
			}
		}


		&.white{
			background: #fff;

			.text{
				color: #000;
			}

			&::after{
				background: #000;
			}

			&.hover{
				span.text{
					color: #fff;
				}
			}

			&.out{
				span.text{
					color: #000;
				}
			}
		}

	}
	/* mi-button END */

	.background_img{
		img{
			width: 100%;
		}
		&#customise_top{
			background: url('/images/adidas/event/customise/170905/01.jpg') no-repeat;
			background-position: center center;
			background-size: cover;
		}
		.bg{
			position: relative;
			width: 100%;
			padding-bottom: 140%;
			background-position: center center;
			background-size: cover;
			z-index: 0;
			.bg1{
				position: absolute;
				top:0;
				left:0;
				width: 100%;
				z-index: 1;
				background-size: cover;
				visibility: hidden;
				padding-bottom: 140%;
				z-index: 1;
				background: url('/images/adidas/event/customise/170905/02.jpg') no-repeat;
				background-position: center center;
				background-size: cover;
			}
			.bg2{
				position: absolute;
				top:0;
				left:0;
				width: 100%;
				z-index: 1;
				background-size: cover;
				visibility: hidden;
				padding-bottom: 140%;
				z-index: 1;
				background: url('/images/adidas/event/customise/170905/03.jpg') no-repeat;
				background-position: center center;
				background-size: cover;
			}
		}
	}

	.masthead{
		width: 100%;
		padding-bottom: 128%;
		position: relative;
		&.android{
    		padding-bottom: 135%;
		}

		.background_img{
			width: 100%;
			position: absolute;
			top: 0;
			left: 0;

			&.bg1{
				top: 0;
				left: 0;
			}

			&.bg2{
				top: 0;
				right: 0;
			}
		}
		.contents{
			width: 100%;
			height: 100%;
			position: absolute;
			top: 0;
			left: 0;
			z-index: 5;

			.copy_wrapper{
				width: 100%;
				position: absolute;
				top:20px;
				left: 0;
				text-align: center;

				h3{
					margin-bottom: 0.5rem;
				    font: 1.8rem/2rem 'adinenuPro-bold';
				    color: #fff;
				    letter-spacing: 5px;
				    text-align: center;
				}

				.desc{
					margin-bottom: 18rem;
				    font-size: 1.3rem;
				    color: #fff;
				    text-align: center;
					font-weight: 300;
				}

				&.black{
					h3{
						color:#000;
					}
					.desc{
						color:#000;
					}
				}

				.video_button{
					display: block;
					width: 8rem;
					height: 8rem;
					position: absolute;
					top: -1rem;
					left: 50%;
					border: 1px solid  #fff;
					border-radius: 50%;

					@include css3(transform,translate3d(-50%,-100%,0));

					.arrow{
						width: 1.8rem;
						height: 2rem;
						border-left: 1px solid  #fff;
						position: absolute;
						top: 50%;
						left: 50%;
						margin-left: 2px;
						@include css3(transform,translate3d(-50%,-50%,0));

						&::before , &::after{
							content:'';
							display: block;
							width: 100%;
							border-top: 1px solid  #fff;
							position: absolute;
							top: 50%;
							left: -0.25rem;
							@include css3(transform,translate3d(0,-50%,0) rotate(30deg));
							@include css3(transform-origin,top right);
						}

						&::after{
							@include css3(transform,translate3d(0,-50%,0) rotate(-30deg));
						}
					}
				}
			}

			.img_wrapper{
				position: absolute;
				top: 90px;
				right: -60px;
				z-index: 4;

				.box{
					position: absolute;
					top: 0;
					right: -90px;

					&.video{
						right: 0;
						margin-top: 75px;
						position: relative;
						z-index: 3;
					}
				}
			}

		}
		.mi-button{
			position: absolute;
			bottom:7%;
			left:50%;
			margin-left: -100px;
			&.border{
				background: transparent;
				border:1px solid #000;
				span{
					color:#000;
				}
			}
		}
	}
	/* masthead END */

	.features_wrapper{
		width: 100%;
		position: relative;
		background:url('//imagem.adidas.co.kr/images/adidas/event/customise/170905/features_bg_tubular.jpg') 0 45% no-repeat;
		background-size: 100% auto;

		.background_img{
			width: 100%;
			position: absolute;
			top: 0;
			left: 0;

			img{
				width: 100%;
			}
		}

		.contents{
			width: 100%;
			margin: 0 auto;
			position: relative;
			overflow: hidden;
			.bg{
				img{
					width: 100%;
				}
			}
			.copy_wrapper{
				position: absolute;
			    left:50%;
				top:78%;
			    width: 87%;
			    // margin: 0 1rem;
				@include css3(transform, translate(-50%,-50%));
				>*{
					text-align: center;
					color:#fff;
				}
				h3{
					font: 2.6rem 'adinenuPro-black';
					letter-spacing: 2px;
				}
				.desc{
					margin-bottom: 30px;
					font:400 1.35rem/1.35rem 'NotoSansKR';
					color: #fff;
					letter-spacing: 0.75px;
					text-transform: uppercase;
				}

				.button_area{
					.mi-button{
						width: 240px;
					    position: relative;
					    display: block;
					    border: 1px solid #000;
					    margin: 0 auto;
					}
				}
			}


		}
	}
	/* features_wrapper END */
	/* sns_wrapper */
	.sns_wrapper{
		position: relative;
		padding: 20px 0 80px;
		.box{
			padding: 50px 0 0 0;
			text-align: center;
			>*{
				text-align: center;
				color:#333;
				letter-spacing: 2px;
			}
			h4{
				padding: 20px 0 5px 0;
				font:2.5rem/2.4rem 'adinenuPro-black';
			}
			p{
				font:400 12px 'NotoSansKR';
				letter-spacing: 0;
			}
			a{
				margin: 10px 0 0 0;
				display: inline-block;
				width: 150px;
				font:400 12px/32px 'NotoSansKR';
				letter-spacing: 0;
				border:1px solid #333;

			}
		}
	}
	/* sns_wrapper END*/
	.mi_products{
		position: relative;
		padding: 0 0 5rem;

		.mi_products_head{
			width: 100%;
			padding: 5rem 0 0;
			background: #fafafa;

			h4{
				margin-bottom: 1rem;
				padding: 0 1rem;
				font-size: 3.2rem;
				line-height: 3.8rem;
				letter-spacing: 0.3rem;
				margin-bottom: 35px;
				font:italic 48px/48px 'adinenuPro-black';
				color:#333;
				text-align: center;
			}

			.desc{
				padding: 0 1rem;
				margin-bottom: 1.5rem;
				font:400 1.3rem/2rem 'NotoSansKR';
				text-align: center;
				color: #5c5c5c;
				letter-spacing:2px;
				word-break:keep-all;
			}

			.mi_products_tab{
				padding-left: 0.05%;
				margin-top:80px;
				text-align: center;

				a{
					width: 50%;
					padding: 2rem 0;
					float: left;
					display: inline-block;
					text-align: center;
					font:1rem/1rem 'adinenuPro-bold';
					letter-spacing: 3px;
					color: #c6c6c6;

					&.on{
						color: #000;
						background: #fff;
					}
				}
			}
		}


		// .desc

		.prodlist{
			text-align: center;

			.inner{
				width: 100%;
				min-width: 100%;
				text-align: center;
				margin:0 auto;
			}
			// .inner

			.item_container{
				width: 100%;
				position: relative;
				padding-top: 3rem;
				display: -webkit-flex;
				display: -ms-flex;
				display: flex;
				flex-wrap:wrap;
				-webkit-flex-wrap:wrap;
				justify-content : center;
				-webkit-justify-content : center;

				&::after{
					content:'';
					display: block;
					width: 100%;
					height: 70%;
					background:linear-gradient(to bottom,#fff 0,#f7f7f7 40%,#f7f7f7 85%,#fff 85%);
					position: absolute;
					top: 0;
					left: 0;
				}
			}

			.item{
				flex:0 0 50%;
				-webkit-flex:0 0 50%;
				display: block;
				position: relative;
				display: inline-block;
				opacity: 0;
				position: relative;
				z-index: 5;

				@include css3(transition , opacity 0.3s ease);

				&.loaded{
					opacity: 1;
				}

				a{
					display: block;
					position: relative;
				}

				.img{
					width: 100%;
					padding-bottom: 50%;
					position: relative;
					text-align: center;
					margin-bottom: 1.5rem;

					.loading-container{
						background: none;
						position: absolute;
						z-index: 2;

						.loader-icon{
							width: 0.8rem;
							height: 0.8rem;
							border-width: 3px;
						}
					}


					img{
						position: absolute;
						top: 0;
						left:50%;
						width: 80%;
						max-width:370px;
						opacity: 0;
						z-index: 5;

						@include css3(transition , opacity 0.3s ease);
						@include css3(transform , translate3d(-50%,0,0));

						&.loaded{
							opacity: 1;
						}
					}
				}

				.button{
					width: 200px;
					height: 45px;
					position: relative;
					top: 0;
					left: 0;
					margin: 0 auto;
					text-transform:uppercase;
					opacity: 1;

					@include css3(transition , all 0.3s ease);

					.arrow{
						&:before{
							background: #fff;
						}
					}
				}

				.mi-button{
					width: 60%;
					max-width: 240px;
					height: auto;
					position: relative;
					text-transform:uppercase;
					font:400 1.1rem/1.1rem 'NotoSansKR';
					background: none;
					border: 1px solid  #000;
					opacity: 1;

					@include css3(transform , translate3d(0,15px,0));
					@include css3(transition , all 0.3s ease);

					.arrow{
						width: 40px;
					    height: 10px;
						margin-left: 25px;
					    position: relative;
					    display: inline-block;
					    background: url('http://image.adidas.co.kr/images/adidas/event/customise/miadidas-interface.png') -13px -45px;

						@include css3(transition,background 0s ease 0.15s);

						&::before , &::after{
							display: none;
						}
					}

					.text{
						position: relative;
						top: 0;
						opacity: 1;
						font-size: 1.2rem;
					}

					&.hover{
						.arrow{
							background-position:-55px -33px;
						}
					}

					&.out{
						.arrow{
							background-position:-13px -45px;
						}
					}
				}

				.text{
					width: 100%;
					padding: 10px;
					position: relative;
					opacity: 1;
					top: 0;
					left: 0;
					font:1rem/1.35rem 'adinenuPro-black';
					color: #000;
					text-transform: uppercase;
					letter-spacing: 3px;
					word-break: keep-all;
					box-sizing: border-box;
					-webkit-box-sizing: border-box;
				}

			}
			// .item
		}
		// .prodlist

		.button_area{
			width: 100%;
			padding-top: 5rem;
			position: relative;
			bottom: 0;
			left: 0;
			text-align: center;


			&.howto_mi{
				margin-bottom: 3rem;
				padding-top: 0;
				position: relative;
				bottom: 0;

				.mi-button{
					width: 100%;
				}
			}

			.mi-button{
				width: 60%;
				max-width: 240px;
				margin: 0 auto;
				line-height: 1.3rem;

				.arrow{
					width: 40px;
					height: 10px;
					margin-left: 25px;
					position: relative;
					display: inline-block;
					background: url('http://image.adidas.co.kr/images/adidas/event/customise/miadidas-interface.png') -55px -33px;

					@include css3(transition,background 0s ease 0.15s);

					&::before , &::after{
						display: none;
					}
				}

				.text{
					font-size: 1.2rem;
				}

				&.hover{
					.arrow{
						background-position:-13px -45px;
					}
				}

				&.out{
					.arrow{
						background-position:-55px -35px;
					}
				}
			}
		}
	}
	/* End of mi_products */



