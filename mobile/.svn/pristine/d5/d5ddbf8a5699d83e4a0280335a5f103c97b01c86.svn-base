		#zprint .form_contents h3{
			font:40px 'QStencil';
			color: #eb1676;
			text-align: center;
		}

		#zprint .form_contents::before{
			content:'';
			display: block;
			width: 100%;
			height: 100%;
			background:rgba(0,0,0,0.84);
			position: absolute;
			top: 0;
			left: 0;
		}

		#zprint .form_contents>.inner{
			position: relative;
			padding: 30px 15px;
		}

		#zprint .form_contents>.inner>p{
			color: #fff;
			text-align: center;
		}

		#zprint .form_contents>.inner>p.desc{
			margin-bottom: 5px;
			font:19px/22px 'NotoSansKRLight';
		}

		#zprint .form_contents>.inner>p.desc strong{
			font:19px/22px 'NotoSansKRBold';
		}

		#zprint .form_contents>.inner>p.date{
			margin-bottom: 15px;
			font:14px 'NotoSansKRLight';
