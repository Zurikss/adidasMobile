		#zprint .form_contents>.inner>p.date span{
			font:16px 'NotoSansKR';
		}

		#zprint .form_contents>.inner>p.date strong{
			font:16px 'NotoSansKRBold';
		}

		.ipt_box>div{
			margin-bottom: 5px;
		}

		.ipt_box>div:last-child{
			margin-bottom: 15px;
		}

		.ipt_box>div input{
			width: 100%;
			height: 40px;
			padding-left: 20px;
			background:none;
			border:2px solid #fff;
			box-sizing: border-box;
