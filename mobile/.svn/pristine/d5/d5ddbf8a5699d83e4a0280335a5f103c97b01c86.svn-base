		#zprint .agree_chk label,
		#zprint .agree_chk_all label{
			position: relative;
			display: inline-block;
			color: #fff;
			font:12px 'NotoSansKR';
			vertical-align: middle;
			padding-left:18px;
		}

		#zprint .agree_chk label::before,
		#zprint .agree_chk_all label::before{
			position: absolute;
			top:3px;
			left: 0;
			display: block;
			content:'';
			width: 12px;
			height: 12px;
			border: 1px solid  #fff;
			transition:background 0.2s;
			-webkit-transition:background 0.2s;
		}

		#zprint .agree_chk_all label{
			padding-left:21px;
		}

		#zprint .agree_chk_all label::before{
			width: 15px;
			height: 15px;
		}

		#zprint .agree_chk input[type="radio"] + label::before,
		#zprint .agree_chk_all input[type="radio"] + label::before{
			border-radius:12px;
		}

		#zprint .agree_chk input:checked + label::before,
		#zprint .agree_chk_all input:checked + label::before{
			background: #fff;
		}

		#zprint .agree_chk input,
		#zprint .agree_chk_all input{
			opacity: 0;
			position: absolute;
			top: 0;
			left: 0;
			vertical-align: middle;
		}

