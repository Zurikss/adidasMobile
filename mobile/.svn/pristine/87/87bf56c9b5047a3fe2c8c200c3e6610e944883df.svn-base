			<script type="text/javascript">

				(function($){

                    // prd ready
                    $('.readyPrd').click(function(){
                        alert('상품이 준비중입니다.');
                        return false
                    })

                    //floting menu click
                    $('#nmd .floting_gnb').click(function(){
                        $('#nmd .floting_wrap').toggleClass('show');
                    })
                    //floting pos show event
                    var showH = $('#nmd .top').height();
                    // var menuH = $('#nmd .menu').height();
                    // console.log(menuH)

                    $(window).scroll(function(){
                        var thisPos = $(this).scrollTop();
                        if(thisPos>showH+30){
                            $('#nmd .floting_wrap').addClass('flotingshow');
                        }else{
                            $('#nmd .floting_wrap').removeClass('flotingshow');
                        }
                    })
                    $(document).ready(function(){
                        $('#nmd .menu ul>li').eq(0).addClass('on');
                        $('#nmd .content_menu ul>li').eq(0).addClass('on');
                    })

                    //menu active


                    $('#nmd .menu ul>li').on('click', function(){
                        var index = $(this).index();
                        $('#nmd .menu ul>li').removeClass('on').eq(index).addClass('on');
                        $('.content_menu ul>li').removeClass('on').eq(index).addClass('on');


                        $('.owl-dots').find('.owl-dot:eq(' + index + ')').trigger('click');

                    });

                    var owlSlider = $('#slider_owl');

                    owlSlider.on('changed.owl.carousel', function(event) {
                        var index = $(event.currentTarget).find('.owl-stage .owl-item').eq(event.item.index).find('li').attr('data-index');
                        $('.menu ul>li').removeClass('on').eq(index).addClass('on');
                        $('.content_menu ul>li').removeClass('on').eq(index).addClass('on');

					});

                    owlSlider.owlCarousel({
                        stagePadding: 0,
                        margin:15,
                        items : 1,
                        merge: true,
                        loop : true,
                        navigation: true,
                        nav: true
                    });

                    $('.popBtn').click(function(){
                        var imgsrc = this.dataset.url;
                        var html = '<div class="layerpopup show">';

                        html+= '<div class="inner">'
                        html+='<a href="javascript:void(0)" class="close_x_btn"></a>'
                        html += '<img src='+ imgsrc +' />';
                        html += '</div>';
                        html += '</div>';
                        $('#nmd').append(html);
                        $('.close_x_btn').click(function(){
                            $('.layerpopup').removeClass('show');
                            $('.layerpopup').remove();
                        })
                    });




				})(jQuery);

			</script>

