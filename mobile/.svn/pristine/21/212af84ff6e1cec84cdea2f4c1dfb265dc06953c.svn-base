                    //셋타임아웃
                    setTimeout(function(){
                        $('.pod_system .box1 .bg_shoe').addClass('on');
                    },1000)

                    $('.pod-modal-popup').hide();
                    function wrapWindowMask(){
                        var maskWidth = $(window).width();
                        var maskHeight = $(document).height();

                        $('.mask').css({'width':maskWidth,'height':maskHeight});
                        $('.mask').fadeTo("fast",0.9);
                        $('.pod-modal-popup').show();
                    }

                    $('.close').click(function(e){
                        e.preventDefault();
                        $('.pod-modal-popup').hide();
                    })

                    $('.mask').click(function(){
                        $('.pod-modal-popup').hide();
                    });

                    $('.grid_wrap .img').bind('click',function(){
                        var dataImg = $(this).attr('data-img');
                        wrapWindowMask()
                        $('.pod-modal-popup .popup-content .popup-img img').attr('src',dataImg);
                    });
                })(jQuery);


            //슬라이더
            (function($){

                var owlSlider = $('#slider_owl');

                owlSlider.on('initialized.owl.carousel', function(event) {
                    /*
                        최초 로딩 시 한번만 호출하는 함수
                    */
                });

                owlSlider.on('changed.owl.carousel', function(event) {
                    var index = event.item.index;
                    console.log(index)
                    $('#slider_owl .owl-item li').removeClass('on').eq(index).addClass('on');
                    /*
                        Callback 이벤트 (슬라이드가 실행 될 떄마다 실행되는 함수)
                        * index 사용하기
                        event.item.index

                        * length 사용하기
                        event.item.count

                    */


                });

                owlSlider.owlCarousel({
                    stagePadding: 0,
                    margin:0,
                    items : 1,
                    merge: true,
                    loop : true,
                    navigation: true,
                    nav: true
                });
            })(jQuery);

        </script>

