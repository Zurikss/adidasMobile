						for(var pos = 1; pos<scrollArray.length; pos++){
							if(thispos>scrollArray[pos]){
								$('#golf_stylebook .contents_wrap.on .scroll').eq(pos).addClass('on');
							}
						}
					});

				});

				// content effect
				var scrollArray = [];
				var selectContnt = $('#golf_stylebook .contents_wrap').eq(0);
				$('#golf_stylebook .contents_wrap .scroll').removeClass('on');

				var scrollContent = selectContnt.find('.scroll');
				for(var i = 0; i<scrollContent.length; i++){
					 var mypos = scrollContent[i].offsetTop;
