	<script type="text/javascript">
		(function($) {

			var owlSlider2 = $('#slider_owl2');

			owlSlider2.owlCarousel({
				stagePadding: 0,
				margin:0,
				items : 1,
				merge: true,
				loop : true,
				navigation: true,
				nav: true,
				autoplay : true,
				autoplayTimeout : 5000,
			});

			var owlSlider = $('#slider_owl');

			owlSlider.owlCarousel({
				stagePadding: 0,
				margin:0,
				items : 1,
				merge: true,
				loop : true,
				navigation: true,
				nav: true,
				autoplay : true,
				autoplayTimeout : 5000,
			});

			var scrollArray = [];
			var scrollCon = document.querySelectorAll('.scrollbox');

			for(var i = 0; i < scrollCon.length; i++){
				var mypos = scrollCon[i].offsetTop;
				scrollArray.push(mypos);
			}
			$(window).scroll(function(){
				for(var pos = 0; pos < scrollArray.length; pos++){
					var thisPos = $(this).scrollTop() + 300 ;
					if(thisPos >= scrollArray[pos]){
						$('.scrollbox').eq(pos).addClass('on');
					}
				}
			});

		})(jQuery);
	</script>

